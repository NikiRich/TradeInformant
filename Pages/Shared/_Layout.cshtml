<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TradeInformant</title>
    <link rel="stylesheet" href="~/css/_Layout.css">
</head>
<body>
    <header>
        <nav>
            <ul>
                <li>
                    <a class="btn-primary" asp-area="" asp-page="/Stocks">Stocks</a>
                    <a asp-area="" asp-page="/Gainers&Losers">Gainers & Losers</a>
                    <a asp-area="" asp-page="/Crypto">Crypto</a>
                    <a asp-area="" asp-page="/Indicators">US Economic Indicators</a>
                    <a asp-area="" asp-page="/About">About Us</a>
                </li>
            </ul>
        </nav>
    </header>
    <main>
        @RenderBody()
    </main>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
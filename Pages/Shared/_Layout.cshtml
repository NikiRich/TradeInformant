<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TradeInformant</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/_Layout.css" />

</head>
<body>
    <nav class="navbar navbar-expand-md bg-info bg-gradient d-flex justify-content-evenly">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link text-black" asp-area="" asp-page="/Stocks">Stocks</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-black" asp-area="" asp-page="/Gainers&Losers">Gainers & Losers</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-black" asp-area="" asp-page="/Crypto">Crypto</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-black" asp-area="" asp-page="/US_Economic_Indicators">US Economic Indicators</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-black" asp-area="" asp-page="/About">About Us</a>
            </li>
        </ul>
    </nav>
    <main>
        @RenderBody()
    </main>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>